includes:
    - phpstan-baseline.neon

parameters:
    level: 5
    paths:
        - src
        - config
        - database
    tmpDir: build/phpstan
    checkOctaneCompatibility: true
    checkModelProperties: true
    ignoreErrors:
        # Package config files can use env() - they get published to app's config directory
        - identifier: larastan.noEnvCallsOutsideOfConfig
          path: config/*
